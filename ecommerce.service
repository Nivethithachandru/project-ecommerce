[Unit]
Description=Ecommerce Django Web Service
After=network.target

[Service]
User=root
Group=root
WorkingDirectory=/var/lib/jenkins/workspace/project_Ecommerce
ExecStart=/var/lib/jenkins/workspace/venv/bin/python3 manage.py runserver 0.0.0.0:8011
Environment"PATH=/var/lib/jenkins/workspace/venv"
Restart=always
RestartSec=3

[Install]
WantedBy=multi-user.target
